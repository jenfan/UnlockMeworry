msgstr "Alleen aantal artikelen."

#: includes/wcmenucart-settings.php:162
msgid "Price Only."
msgstr "Alleen prijs."

#: includes/wcmenucart-settings.php:163
msgid "Both price and items."
msgstr "Zowel prijs als aantal artikelen."

#: includes/wcmenucart-settings.php:170
msgid "Select the alignment that looks best with your menu."
msgstr "Kies de uitlijning die het beste bij jouw menu past"

#: includes/wcmenucart-settings.php:178
msgid "Align Left."
msgstr "Links uitlijnen."

#: includes/wcmenucart-settings.php:179
msgid "Align Right."
msgstr "Rechts uitlijnen."

#: includes/wcmenucart-settings.php:180
msgid "Default Menu Alignment."
msgstr "Standaard uitlijning."

